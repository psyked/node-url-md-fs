{
  "name": "url-md-fs",
  "version": "0.5.0",
  "description": "Enter an URL, convert it to Markdown, save it to the File system",
  "main": "src/index.js",
  "bin": {
    "url-md-fs": "./src/index.js"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint .",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major",
    "preversion": "node ./scripts/preversion.js",
    "postversion": "node ./scripts/postversion.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psyked/node-url-md-fs.git"
  },
  "keywords": [
    "markdown",
    "filesystem",
    "convert",
    "download"
  ],
  "author": "James Ford <psyked84@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/psyked/node-url-md-fs/issues"
  },
  "homepage": "https://github.com/psyked/node-url-md-fs#readme",
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jest": "^23.4.0",
    "jest-raw-loader": "^1.0.1",
    "prettier": "^1.13.7",
    "shelljs": "^0.8.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "image-downloader": "^3.4.1",
    "turndown": "^4.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.html$": "jest-raw-loader",
      "\\.md$": "jest-raw-loader"
    }
  }
}
